{
    "require": {
        "php-di/php-di": "^6.2",
        "selective/basepath": "^1.0",
        "slim/php-view": "^3.0",
        "slim/psr7": "^1.2",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
